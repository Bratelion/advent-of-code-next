{
  "extends": [
    "next/core-web-vitals",
    "plugin:jsx-a11y/recommended",
    "plugin:fp/recommended"
  ],
  "plugins": ["fp", "jsx-a11y"],
  "env": {
    "browser": true
  },
  "rules": {
    "fp/no-unused-expression": "off",
    "fp/no-nil": "off"
  },
  "ignorePatterns": ["*.d.ts", "_document.tsx", "_app.tsx"]
}
